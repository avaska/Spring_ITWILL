<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<!-- community table SQL 쿼리문 저장소 -->

<sqlMap namespace="Community">
	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="mem" type="com.member.db.MemberDTO" />
	
	<select id="getUserNick" parameterClass="str" resultClass="mem">
		<![CDATA[
			SELECT * FROM vancomember WHERE userId = #userId#
		]]>
	</select>
	
</sqlMap>